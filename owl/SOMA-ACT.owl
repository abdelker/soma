    <SubClassOf>
        <Annotation>
            <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
            <Literal>If a SituationTransition manifests in some Event, then it also has an initial Situation and a terminal one.</Literal>
        </Annotation>
        <ObjectIntersectionOf>
            <Class IRI="http://www.ease-crc.org/ont/SOMA.owl#SituationTransition"/>
            <ObjectSomeValuesFrom>
                <ObjectProperty IRI="http://www.ease-crc.org/ont/SOMA.owl/manifestsIn"/>
                <Class IRI="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
            </ObjectSomeValuesFrom>
        </ObjectIntersectionOf>
        <ObjectIntersectionOf>
            <ObjectSomeValuesFrom>
                <ObjectProperty IRI="http://www.ease-crc.org/ont/SOMA.owl#hasInitialSituation"/>
